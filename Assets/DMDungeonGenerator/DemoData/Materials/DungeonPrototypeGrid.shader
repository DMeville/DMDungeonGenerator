			float4 triplanar22 = TriplanarSampling22( _Texture0, ase_worldPos, ase_worldNormal, _TilingFalloff.y, temp_cast_0, 1.0, 0 );
			o.Albedo = ( triplanar22 * _Color ).xyz;
